
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/c000000.wifi'
	option country 'US'
	option cell_density '3'
	option htmode 'HE80'
	option band '5g'
	option channel '36'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/c000000.wifi+1'
	option channel 'auto'
	option band '2g'
	option htmode 'HE40'
	option country 'US'
	option cell_density '1'
	option noscan '1'
	option txpower '30'

config wifi-iface 'lan_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrt_WiFi'
	option encryption 'psk2+ccmp'
	option key '123456789'
	option network 'lan'
	option ieee80211r '1'
	option ieee80211w '1'
	option ieee80211k '1'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option ft_psk_generate_local '1'
	option ft_over_ds '0'
	option na_mcast_to_ucast '1'
	option wnm_sleep_mode_no_keys '1'
	option proxy_arp '1'

config wifi-iface 'lan_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'OpenWrt_WiFi'
	option key '123456789'
	option encryption 'psk2+ccmp'
	option network 'lan'
	option ieee80211r '1'
	option ieee80211w '1'
	option ieee80211k '1'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option ft_psk_generate_local '1'
	option ft_over_ds '0'
	option na_mcast_to_ucast '1'
	option wnm_sleep_mode_no_keys '1'
	option proxy_arp '1'

