[global]
        netbios name = OPENWRT
        server string = OPENWRT
        workgroup = |WORKGROUP|
        interfaces = |INTERFACES|
        bind interfaces only = yes
        ipc timeout = 20
        deadtime = 15
        map to guest = never 
        cache read buffers = no
        cache trans buffers = no
        server min protocol = SMB3_11
        server multi channel support = yes
