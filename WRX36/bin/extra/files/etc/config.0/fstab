
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '1'

config mount
	option target '/mnt/PLEX'
	#option uuid ''
	option enabled '1'
	option fstype 'ntfs-3g'

config mount
	option target '/tmp/plexmediaserver'
	#option uuid ''
	#option enabled '1'
	option fstype 'squashfs'

config swap
	option enabled '1'
	option device '/dev/zram0'

config uvol
	option initialized '1'

