
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount
	option target '/tmp/plexmediaserver'
	option uuid 'df88b5aa-9668b6c0-058b0467-80acf339'
	option enabled '1'
	option fstype 'squashfs'

config mount
	option target '/mnt/BaroneHouse'
	option uuid '4A8E7EA68E7E8A69'
	option enabled '1'
	option fstype 'ntfs-3g'

config swap
	option enabled '1'
	option device '/dev/zram0'

config uvol
	option initialized '1'

