[global]
	netbios name = |NAME|
	server string = |DESCRIPTION|
	workgroup = |WORKGROUP|
	interfaces = |INTERFACES|
	bind interfaces only = yes
	ipc timeout = 20
	deadtime = 15
	map to guest = Bad User
	smb2 max read = 4096K
	smb2 max write = 4096K
	smb2 max trans = 1024K
	cache read buffers = no
	cache trans buffers = no
