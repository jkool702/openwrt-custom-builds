/dts-v1/;

/ {
	description = "ARM64 OpenWrt FIT (Flattened Image Tree)";
	#address-cells = <1>;

	images {
		kernel-1 {
			description = "ARM64 OpenWrt Linux-6.6.30";
			data = /incbin/("/mnt/ramdisk/build/openwrt/build_dir/target-aarch64_cortex-a53_musl/linux-qualcommax_ipq807x/zyxel_nbg7815-uImage.itb");
			type = "kernel";
			arch = "arm64";
			os = "linux";
			compression = "gzip";
			load = <0x41000000>;
			entry = <0x41000000>;
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};


		fdt-1 {
			description = "ARM64 OpenWrt zyxel_nbg7815 device tree blob";
			
			data = /incbin/("/mnt/ramdisk/build/openwrt/build_dir/target-aarch64_cortex-a53_musl/linux-qualcommax_ipq807x/image-ipq8074-nbg7815.dtb");
			type = "flat_dt";
			
			arch = "arm64";
			compression = "none";
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};



	};

	configurations {
		default = "config@nbg7815";
		config@nbg7815 {
			description = "OpenWrt zyxel_nbg7815";
			kernel = "kernel-1";
			fdt = "fdt-1";
			
			
			
		};
		
	};
};
